<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Photo" table="photo">
    <indexes>
      <index name="IDX_14B78418A76ED395" columns="user_id"/>
    </indexes>
    <unique-constraints>
      <unique-constraint name="UNIQ_14B7841871F7E88B" columns="event_id"/>
      <unique-constraint name="UNIQ_14B78418EE45BDBF" columns="picture_id"/>
    </unique-constraints>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="position" type="integer" column="position" nullable="false"/>
    <field name="idSource" type="string" column="id_source" length="45" nullable="false"/>
    <field name="dateCreate" type="datetime" column="date_create" nullable="false"/>
    <field name="dateUpdate" type="datetime" column="date_update" nullable="false"/>
    <field name="state" type="enumphotostate" column="state" nullable="false"/>
    <many-to-one field="picture" target-entity="Picture">
      <join-columns>
        <join-column name="picture_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="user" target-entity="User">
      <join-columns>
        <join-column name="user_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="event" target-entity="Event">
      <join-columns>
        <join-column name="event_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
